import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner scanner = new Scanner(System.in);

		System.out.println("Sayý giriniz");
		int sayi = scanner.nextInt();
		int prev;
		int first = 0;
		int sum = 0;
		for(int i = 0; i<=sayi; i++) {
			if (i == 0 ) {
				System.out.println(sum);
			} else {
				sum = sum + i;
				System.out.println(sum);
			}
		}
	}

}

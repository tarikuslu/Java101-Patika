import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		
		System.out.println("taban sayý giriniz: ");
		int tabanSayi = scanner.nextInt();
		
		System.out.println("üs giriniz: ");
		int usSayi = scanner.nextInt();
		
		System.out.println(usAlma(tabanSayi, usSayi));
	}

	
	static int usAlma(double taban, int us) {
		if (us == 1) {
			return Math.pow(taban, 1);
		}
		
		usAlma(taban, us - 1);
	}
}

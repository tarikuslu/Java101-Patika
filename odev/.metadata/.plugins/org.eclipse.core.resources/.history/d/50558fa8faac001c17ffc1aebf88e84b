import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner scanner = new Scanner(System.in);
		
		System.out.println("Yýl giriniz: ");
		int yil = scanner.nextInt();
		
		if (yil % 4 == 0) {
			if (yil % 100 == 0) {
				System.out.println("Artýk yýl!");
			}
		}

	}

}
